<module>
	<inherits name="com.foxtox.rpc.client.RpcClient" />
	<inherits name="com.foxtox.rpc.client.gwt.RpcClientGwt" />
	<inherits name="com.foxtox.rpc.common.RpcCommon" />
	<inherits name="com.google.gwt.user.User" />

	<source path="" />

	<entry-point class="com.foxtox.rpc.client.example.GwtEntryPoint" />
	
	<generate-with class="com.foxtox.rpc.rebind.RemoteServiceClientGenerator">
		<when-type-assignable class="com.foxtox.rpc.client.RemoteService" />
	</generate-with>
</module>